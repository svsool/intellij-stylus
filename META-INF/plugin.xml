<idea-plugin version="2">
	<id>org.intellij.stylus</id>
	<name>Stylus Support</name>
	<version>1.0</version>
	<vendor email="svsool@bk.ru" url="http://www.yourcompany.com">svsool &amp; VISTALL</vendor>

	<extensions defaultExtensionNs="com.intellij">
		<fileTypeFactory implementation="com.intellij.lang.stylus.StylusFileTypeFactory"/>
		<lang.parserDefinition language="Stylus" implementationClass="com.intellij.lang.stylus.StylusParserDefinition"/>
		<lang.syntaxHighlighterFactory key="Stylus" implementationClass="com.intellij.lang.stylus.editor.StylusSyntaxHighlighterFactory"/>
		<colorSettingsPage implementation="com.intellij.lang.stylus.editor.StylusColorSettingsPage"/>
		<highlightVisitor implementation="com.intellij.lang.stylus.editor.highlight.StylusHighlightVisitor"/>
	</extensions>
</idea-plugin>